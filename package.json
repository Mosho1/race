{
  "name": "react-mobx-boilerplate",
  "version": "0.0.1",
  "description": "A react boilerplate project featuring webpack 2, HMR, code splitting, mobx, SSR",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node webpack/webpack-dev-server --env.dev",
    "start": "npm run build && cp -r src/images build && node prod-server.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js --progress --env.prod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mosho1/react-mobx-boilerplate.git"
  },
  "author": "Sharon Rolel <sharonrolel@gmail.com>",
  "homepage": "https://github.com/Mosho1/react-mobx-boilerplate",
  "dependencies": {
    "@types/pixi.js": "^4.5.4",
    "@types/react": "^15.0.38",
    "@types/react-dom": "^15.5.1",
    "@types/react-hot-loader": "^3.0.3",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.2.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "json-loader": "^0.5.7",
    "mobx": "^3.2.1",
    "mobx-react": "^4.2.2",
    "mobx-react-devtools": "^4.2.15",
    "pixi.js": "^4.5.6",
    "postcss": "^6.0.6",
    "postcss-loader": "^2.0.6",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-hot-loader": "3.0.0-beta.6",
    "routes": "^2.1.0",
    "static-server": "^2.0.5",
    "style-loader": "^0.18.2",
    "tslib": "^1.7.1",
    "typescript": "^2.4.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.5.1"
  }
}
